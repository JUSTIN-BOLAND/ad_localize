<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <%- singulars.each do |translation| -%>
  <%- if translation.comment -%>
  <!-- <%= translation.comment %> -->
  <%- end -%>
  <string name="<%= translation.key.label %>"><%= translation.value %></string>
  <%- end -%>

  <%- plurals.each do |label, translations| -%>
  <plurals name="<%= label %>">
    <%- translations.each do |translation| -%>
    <item quantity="<%= translation.key.plural_key %>"><%= translation.value %></item>
    <%- end -%>
  </plurals>
  <%- end -%>
</resources>