<?xml version="1.0" encoding="UTF-8"?>
<plist>
  <dict>
    <%- plurals.each do |label, translations| -%>
    <key><%= label %></key>
    <dict>
      <key>NSStringLocalizedFormatKey</key>
      <string>"%\#@key@"</string>
      <key>key</key>
      <dict>
        <key>NSStringFormatSpecTypeKey</key>
        <string>NSStringPluralRuleType</string>
        <key>NSStringFormatValueTypeKey</key>
        <string>d</string>
        <%- translations.each do |translation| -%>
        <key><%= translation.key.plural_key -%></key>
        <string><%= translation.value -%></string>
        <%- end -%>
      </dict>
    </dict>
    <%- end -%>
    <%- adaptives.each do |label, translations| -%>
    <key><%= label %></key>
    <dict>
      <key>NSStringVariableWidthRuleType</key>
      <dict>
        <%- translations.each do |translation| -%>
        <key><%= translation.key.adaptive_key %></key>
        <string><%= translation.value %></string>
        <%- end -%>
      </dict>
    </dict>
    <%- end -%>
  </dict>
</plist>
